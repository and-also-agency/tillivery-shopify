/* Tooltip container */
.tooltip2 {
  position: relative;
  text-align:center;
  color:#8398a4;
  font-size:15px;
  margin-bottom:13px;
  z-index:999999;
}

/* Tooltip text */
.tooltip2 .tooltiptext2 {
  visibility: hidden;
  width: 200px;
  background-color:#415966;
  color: #fff;
  text-align: center;
  padding: 10px 10px;
  border-radius: 6px;
  font-weight:600;
  top: 100%;
  left: 50%; 
  margin-left: -100px; 
  position: absolute;
  z-index: 1;
}

/* Show the tooltip text when you mouse over the tooltip container */
.tooltip2:hover .tooltiptext2 {
  visibility: visible;
}


